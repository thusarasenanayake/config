127.0.0.1	localhost
127.0.1.1	linux
127.0.0.1	ubuntu
# The following lines are desirable for IPv6 capable hosts
::1     ip6-localhost ip6-loopback
fe00::0 ip6-localnet
ff00::0 ip6-mcastprefix
ff02::1 ip6-allnodes
ff02::2 ip6-allrouters

## Local - Start ##
::1 myqr.local #Local Site
::1 www.myqr.local #Local Site
127.0.0.1	www.myqr.local #Local Site
::1 kkl.local #Local Site
127.0.0.1	kkl.local #Local Site
::1 www.kkl.local #Local Site
127.0.0.1	www.kkl.local #Local Site
## Local - End ##
